# Install Visual Studio Code
RUN apt-get install -y -qq \
      libgtk2.0-0 \
      libgconf-2-4 \
      libnss3 \
      libasound2 \
      libxtst6 \
      libnotify4
ADD https://vscode-update.azurewebsites.net/latest/linux-deb-x64/stable \
    /tmp/vscode-amd64.deb
RUN dpkg -i /tmp/vscode-amd64.deb && rm /tmp/vscode-amd64.deb
 
